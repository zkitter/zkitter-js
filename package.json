    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url": "^0.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=16 <=18"
  },
  "esModuleInterop": true
}
